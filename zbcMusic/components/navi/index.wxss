.container {
  width: 600rpx;
  height: 80rpx;
  background-color: #f7f7f7;
  border-radius: 2px;
  display: inline-flex;
  justify-content: space-between;
  align-items: center;
}

.container .icon {
  width: 80rpx;
  height: 80rpx;
}

.container .title {
  width: 440rpx;
  text-align: center;
  font-size: 28rpx;
}
